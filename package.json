{
  "name": "tTrackr",
  "productName": "tTrackr",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng b && npm run electron",
    "dev:watch": "concurrently --kill-others-on-fail \"npm run live-reload\" \"npm run build:watch\" --raw",
    "live-reload": "live-reload dist/*.js --port 35729",
    "build": "ng build",
    "build:watch": "ng build -op dist -w -dop=false",
    "test": "ng test",
    "test:ci": "karma start karma-ci.conf.js --single-run",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package-all": "concurrently --kill-others-on-fail \"npm run package-mac\" \"npm run package-win\" \"npm run package-linux\" --raw",
    "create-installer-mac": "electron-installer-dmg ./release-builds/tTrackr-darwin-x64/tTrackr.app tTrackr --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.6",
    "@angular/cdk": "^2.0.0-beta.8",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.8",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "electron": "^1.7.4",
    "hammerjs": "^2.0.8",
    "ng2-date-picker": "^2.1.1",
    "rxjs": "^5.1.0",
    "typedjson-npm": "^0.1.7",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0-beta.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "concurrently": "^3.5.0",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^8.7.2",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "live-reload": "^1.1.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  },
  "main": "main.js"
}
