  "scripts": {
    "ng": "ng",
    "dev:watch": "concurrently --kill-others-on-fail \"npm run live-reload\" \"npm run build:watch\" --raw",
    "live-reload": "live-reload dist/*.js --port 35729",
    "build": "webpack",
    "build:watch": "webpack --output-path dist -w",
    "test": "karma start ./karma.conf.js",
